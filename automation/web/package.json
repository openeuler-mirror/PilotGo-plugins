{
  "name": "automation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --port 4032",
    "build": "vue-cli-service build",
    "type-check": "vue-tsc --build",
    "lint": "eslint src --ext .vue,.ts --fix",
    "format": "prettier --write src/"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add",
      "npm run lint"
    ],
    "src/**/*.vue": [
      "prettier --write",
      "git add",
      "npm run lint"
    ],
    "src/**/*.scss": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.9.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.9",
    "js-cookie": "^3.0.5",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.17.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "typescript": "~5.8.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.8"
  }
}
