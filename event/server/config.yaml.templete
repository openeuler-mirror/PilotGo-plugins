http_server:
  addr: "localhost:8809"
log:
  level: debug
  driver: file #可选stdout和file。stdout：输出到终端控制台；file：输出到path下的指定文件。
  path: ./log/pilotgo_plugin_event.log
  max_file: 1
  max_size: 10485760
influxd:
  url: http://localhost:8086
  token: ""
  organization: "kylin"
  bucket: "test"
  measurement: "test_measurement"
etcd:
  endpoints:
    - "localhost:2379"
  service_name: "event-service"
  version: "3.0"
  dialTimeout: 5s
  menu_name: "事件时间轴"
  icon: "Odometer"