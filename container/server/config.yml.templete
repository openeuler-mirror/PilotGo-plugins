http:
  addr: "0.0.0.0:9999"
mysql:
  host: localhost
  port: 3306
  user: ''
  password: ''
  database: PluginContainer
log:
  level: debug
  driver: stdout    #可选stdout和file。stdout：输出到终端控制台；file：输出到path下的指定文件。
  path: ./log/pilotgo_server.log
  max_file: 1
  max_size: 10485760
etcd:
  endpoints:
    - "localhost:2379"
  service_name: "container-service"
  version: "3.0"
  dialTimeout: 5s
  menu_name: "容器管理"
  icon: "Odometer"